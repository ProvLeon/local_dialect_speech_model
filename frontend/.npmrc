# Memory optimization for build process
maxsockets=5
registry=https://registry.npmjs.org/
fund=false
audit=false

# Reduce memory usage during package installation
cache-max=1073741824
cache-min=10485760

# Build optimization
progress=false
loglevel=error

# Security settings
package-lock=true
save-exact=true

# Performance settings
prefer-offline=true
